<%- include('partials/top.ejs') %> 

<main>
    <h1>MONGO SHOW ALL RECORDS EXAMPLE</h1>

    <p>All records from 'users' collection in the db are shown here.</p>

    <ul>

    <% dbResultsArray.forEach((user)=>{ %>

        <li>
            <li>User ID: <%= user._id %></li>
            <li>Username: <%= user.username %></li>
            <li>Username: <%= user.email %></li>
            <li>Password: <%= user.password %></li>
        </li>

    <% }); %>

    </ul>

</main>

<%- include('partials/bottom.ejs') %> 